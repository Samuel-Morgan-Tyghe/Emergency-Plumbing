    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^1.3.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
